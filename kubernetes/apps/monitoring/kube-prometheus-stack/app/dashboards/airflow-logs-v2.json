      "description": "Shows the actual output from your DAG tasks (print statements, echo commands, logger output)",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "fixed"
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": false,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Ascending",
        "wrapLogMessage": true
      },
      "pluginVersion": "11.3.2",
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "{namespace=\"airflow\", pod=~\".*-${dag_id:pipe}-.*\"} |~ \"subprocess.py.*INFO\" | line_format \"{{.Line}}\" | regexp \"(?P<timestamp>\\\\[[^\\\\]]+\\\\]) \\\\{subprocess.py:[0-9]+\\\\} INFO - (?P<output>.*)\"",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "üéØ Task Output Logs (What Your Tasks Print)",
      "transformations": [
        {
          "id": "extractFields",
          "options": {
            "replace": true,
            "source": "Line"
          }
        }
      ],
      "type": "logs"
    },
    {
      "datasource": {
        "type": "loki",
        "uid": "${datasource}"
      },
      "description": "Python logger.info(), logger.warning(), logger.error() messages from your tasks",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 12
